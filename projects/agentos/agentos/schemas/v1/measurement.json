{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Measurement",
  "type": "object",
  "required": [
    "cls",
    "impf",
    "churn_score",
    "feature_delta"
  ],
  "additionalProperties": false,
  "properties": {
    "cls": {"type": "number"},
    "impf": {"type": "number", "minimum": 0},
    "churn_score": {"type": "number"},
    "feature_delta": {"type": "number"},
    "errors": {
      "type": "array",
      "items": {"type": "string"}
    }
  }
}
